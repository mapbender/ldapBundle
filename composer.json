{
    "name": "mapbender/ldapbundle",
    "description": "Add LDAP Login to Mapbender with FOM as fallback",
    "keywords": ["mapbender","ldap","security"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {"name": "David Patzke"}
    ],
    "require": {
        "ext-ldap": "*",
        "php": ">=5.5",
        "symfony/ldap": "^3.4 || ^4 || ^5 || ^6",
        "mapbender/fom": "~3.1.12 || ^3.2.12"
    },
    "autoload": {
        "psr-4": {"Mapbender\\LDAPBundle\\": "."}
    },
    "extra": {
        "branch-alias": {
            "dev-release/1.0": "1.0.x-dev",
            "dev-master": "1.1.x-dev"
        }
    }
}
